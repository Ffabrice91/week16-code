pipeline {
    agent any
    tool name: 'M2_HOME', type: 'maven'
    stages{
        stage(' clean old  artifacts '){
            steps{
                sh 'mvn clean'
            }
        }
        stage('maven install and package'){
            steps{
                sh 'mvn install '
                sh 'mvn package'
            }
        }
    }
}